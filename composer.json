{
  "name" : "your-vendor/userspace",
  "description" : "Плагин для создания личного кабинета (личного пространства пользователя).",
  "type" : "wordpress-plugin",
  "license" : "GPL-2.0-or-later",
  "authors" : [
    {
      "name" : "Your Name",
      "homepage" : "https://example.com/"
    }
  ],
  "require" : {
    "php" : ">=8.1"
  },
  "autoload" : {
    "psr-4" : {
      "UserSpace\\" : "src/",
      "UserSpace\\Adapters\\" : "adapters/",
      "UserSpace\\Theme\\First\\" : "themes/first/src/",
      "UserSpace\\Theme\\Minimal\\" : "themes/minimal/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UserSpace\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "mockery/mockery": "^1.6"
  }
}
